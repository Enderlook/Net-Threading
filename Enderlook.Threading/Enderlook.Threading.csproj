<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5;net6</TargetFrameworks>
    <OutputType>Library</OutputType>
    <PackageId>Enderlook.Threading</PackageId>
    <AssemblyName>Enderlook.Threading</AssemblyName>
    <RootNamespace>Enderlook.Threading</RootNamespace>
    <Authors>Enderlook</Authors>
    <Product>Enderlook.Threading</Product>
    <RepositoryUrl>https://github.com/Enderlook/Net-Threading/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>0.2.0</Version>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <LangVersion>10</LangVersion>
    <CheckForOverflowUnderflow Condition="'$(Configuration)' == 'Debug'">true</CheckForOverflowUnderflow>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Enderlook.Pools" Version="0.1.0" />
  </ItemGroup>

</Project>